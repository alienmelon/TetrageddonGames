package tetrigeddon {	// Score.as	/**	 * Score keeper.	 */	public class Score	{		private var _score:Number;				public function Score()		{			_score = 0;		}				public function addScore(n:Number)		{			_score += n;		}				public function takeScore(n:Number)		{			_score -= n;		}				public function getScore():Number		{			return _score;		}				public function setScore(n:Number)		{			_score = n;		}	}}